<form method="post" action="{{ path('student_delete', {'id': student.id}) }}" onsubmit="return confirm('Etes-vous sûr de vouloir effacer cette entrée?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ student.id) }}">
    <button class="btn btn-warning">Delete</button>
</form>
