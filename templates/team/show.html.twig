{% extends 'home/index.html.twig' %}

{% block title %}Team{% endblock %}

{% block content %}
    <h1>Team</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ team.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ team.name }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ team.country }}</td>
            </tr>
            <tr>
                <th>Money</th>
                <td>{{ team.money }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('team_index') }}">back to list</a>

    <a href="{{ path('team_edit', {'id': team.id}) }}">edit</a>

    {{ include('team/_delete_form.html.twig') }}
{% endblock %}
